             <foreignAssets>     .TValueList  </foreignAssets>
             <globalNui>         0            </globalNui>
             <globalNbs>         0            </globalNbs>
             <localNui>          0            </localNui>
             <localNbs>          0            </localNbs>
             <extraProgramPages> 0            </extraProgramPages>
