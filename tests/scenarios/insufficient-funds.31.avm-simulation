declareTealSource "clear-basic.teal";
declareTealSource "clear-basic.teal";

addAccount <address> LVMR75YJKH4ATBQ6UFI6IUM2FMU7NNLDQRBKIMG6Q2QAAAXWLIIKWMZRPE </address> <balance> 200000 </balance>;
addAccount <address> MNQJIEWQIGVNYXPWGTVOT7SVEXR7BEGJJJMIHNTO3HSM6QY3QAUYG5PODQ </address> <balance> 10000000 </balance>;

#initTxGroup();

// Pay b"2". This should fail even though later in the same group the funds are given back, because it would
// cause b"1"'s balance to go negative.
addPaymentTx <sender>   LVMR75YJKH4ATBQ6UFI6IUM2FMU7NNLDQRBKIMG6Q2QAAAXWLIIKWMZRPE  </sender>
             <receiver> MNQJIEWQIGVNYXPWGTVOT7SVEXR7BEGJJJMIHNTO3HSM6QY3QAUYG5PODQ  </receiver>
             <amount>   300000 </amount>;

addPaymentTx <sender>   MNQJIEWQIGVNYXPWGTVOT7SVEXR7BEGJJJMIHNTO3HSM6QY3QAUYG5PODQ  </sender>
             <receiver> LVMR75YJKH4ATBQ6UFI6IUM2FMU7NNLDQRBKIMG6Q2QAAAXWLIIKWMZRPE  </receiver>
             <amount>   300000 </amount>;

#initGlobals(); #evalTxGroup(); .AS
